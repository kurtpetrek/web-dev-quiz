{
  "name": "web-dev-quiz",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://kurtpetrek.github.io/web-dev-quiz",
  "dependencies": {
    "gh-pages": "^1.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "react-scripts": "1.0.13",
    "typescript": "^2.5.3",
    "typescript-styled-plugin": "^0.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "babylon",
    "semi": true,
    "tabWidth": 2
  }
